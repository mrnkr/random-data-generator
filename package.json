{
  "name": "random-data-generator",
  "version": "0.0.3",
  "description": "CLI to generate random data in a database",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "watch": "nodemon",
    "test": "jest"
  },
  "repository": "https://github.com/mrnkr/random-data-generator",
  "keywords": [
    "cli",
    "random",
    "database"
  ],
  "author": "Mr.NKR",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.7.0",
    "awilix": "^4.2.0",
    "chalk": "^2.4.2",
    "faker": "^4.1.0",
    "figlet": "^1.2.1",
    "inquirer": "^6.2.1",
    "mongodb": "^3.1.13",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/figlet": "^1.2.0",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "0.0.43",
    "@types/jest": "^23.3.13",
    "@types/mongodb": "^3.1.19",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.12.18",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "typescript": "^3.2.4"
  },
  "bin": {
    "rdg": "./dist/index.js"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts)?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ]
  }
}
